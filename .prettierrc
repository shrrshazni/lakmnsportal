{
    "semi": true,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "overrides": [
        {
            "files": "*.ejs",
            "options": {
                "parser": "html",
                "htmlWhitespaceSensitivity": "ignore"
            }
        }
    ]
}
