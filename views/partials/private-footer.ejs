<script>
  // button loading function
  $(document).ready(function() {
    $('.loading-upload-button').on('click', function() {
      $('.loading-upload-button').addClass('loading-upload');
      $('.content-upload-button').addClass('noshow');

      setTimeout(function() {
        $('.loading-upload-button').removeClass('loading-upload');
        $('.content-upload-button').removeClass('noshow');
      }, 2500);
    });
  });

  $(document).ready(function() {
    $('.loading-button').on('click', function() {
      $('.loading-button').addClass('loading');
      $('.content-button').addClass('noshow');

      setTimeout(function() {
        $('.loading-button').removeClass('loading');
        $('.content-button').removeClass('noshow');
      }, 2500);
    });
  });

  // upload files
  $(document).ready(function() {
    // Function to show the toast
    function showDropzoneFilesToast(content) {
      $('#dropzoneAlertContent').text(content);

      var toast = new bootstrap.Toast(
        document.getElementById('alertUploadToast')
      );
      toast.show();
    }

    var myDropzone = Dropzone.forElement('#dropzone');

    $('#uploadFiles').on('click', function() {
      setTimeout(function() {
        // files upload process queue

        if (myDropzone.getQueuedFiles().length > 0) {
          // If files are present, show the toast with appropriate content
          showDropzoneFilesToast('Files has been uploaded');
          // Process the Dropzone queue
          myDropzone.processQueue();

          //   $('#addFiles').modal('hide');
        } else {
          // If no files are present, show the toast with different content
          showDropzoneFilesToast('No files selected');
        }

        button.prop('type', 'submit');
      }, 2000);
    });
  });
</script>

